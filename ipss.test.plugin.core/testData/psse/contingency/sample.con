COM ---------------------------------------------------------------
COM  Synthetic PSS/E contingency file for unit testing.
COM  All bus numbers and labels are fictitious.
COM ---------------------------------------------------------------

COM --- CATEGORY block ---
CATEGORY 'CAT_N1'
VALUE 'CAT_N2'
END

COM --- 1. Two-terminal branch outage (DISCONNECT keyword) ---
CONTINGENCY 'N1_LINE_A'
DISCONNECT BRANCH FROM BUS 1001 TO BUS 1002 CIRCUIT 1
END

COM --- 2. Two-terminal branch outage (OPEN keyword) ---
CONTINGENCY 'N1_LINE_B'
OPEN LINE FROM BUS 1001 TO BUS 1003 CKT 1
END

COM --- 3. Two-terminal branch outage (TRIP keyword) ---
CONTINGENCY 'N1_LINE_C'
TRIP BRANCH FROM BUS 1002 TO BUS 1003 CIRCUIT 1
END

COM --- 4. Three-winding transformer: all windings ---
CONTINGENCY 'N1_3W_XFR'
DISCONNECT BRANCH FROM BUS 2001 TO BUS 2002 TO BUS 2003 CKT 1
END

COM --- 5. Three-winding transformer: single winding ---
CONTINGENCY 'N1_3W_WINDING'
DISCONNECT THREEWINDING AT BUS 2001 TO BUS 2002 TO BUS 2003 CKT 1
END

COM --- 6. Close (restore) two-terminal branch ---
CONTINGENCY 'CLOSE_LINE'
CLOSE BRANCH FROM BUS 1001 TO BUS 1002 CIRCUIT 1
END

COM --- 7. Close (restore) three-winding transformer ---
CONTINGENCY 'CLOSE_3W_XFR'
CLOSE BRANCH FROM BUS 2001 TO BUS 2002 TO BUS 2003 CKT 1
END

COM --- 8. Bus isolation ---
CONTINGENCY 'DISC_BUS'
DISCONNECT BUS 3001
END

COM --- 9. TRIP BUS (same as DISCONNECT BUS) ---
CONTINGENCY 'TRIP_BUS'
TRIP BUS 3002
END

COM --- 10. Remove generator (MACHINE keyword) ---
CONTINGENCY 'REM_MACHINE'
REMOVE MACHINE G1 FROM BUS 4001
END

COM --- 11. Remove generator (UNIT keyword, synonym for MACHINE) ---
CONTINGENCY 'REM_UNIT'
REMOVE UNIT 1 FROM BUS 4002
END

COM --- 12. Remove load ---
CONTINGENCY 'REM_LOAD'
REMOVE LOAD 1 FROM BUS 5001
END

COM --- 13. Remove fixed shunt ---
CONTINGENCY 'REM_SHUNT'
REMOVE SHUNT 1 FROM BUS 5002
END

COM --- 14. Remove switched shunt ---
CONTINGENCY 'REM_SWSHUNT'
REMOVE SWSHUNT FROM BUS 5003
END

COM --- 15. Remove switched shunt with id ---
CONTINGENCY 'REM_SWSHUNT_ID'
REMOVE SWSHUNT A1 FROM BUS 5004
END

COM --- 16. DECREASE BUS generation ---
CONTINGENCY 'DEC_GEN'
DECREASE BUS 6001 GENERATION BY 100 MW
END

COM --- 17. DECREASE BUS load (REDUCE synonym) ---
CONTINGENCY 'REDUCE_LOAD'
REDUCE BUS 6002 LOAD BY 50 PERCENT
END

COM --- 18. SET BUS shunt ---
CONTINGENCY 'SET_SHUNT'
SET BUS 6003 SHUNT TO 25 MVAR
END

COM --- 19. Move load between buses ---
CONTINGENCY 'MOVE_LOAD'
MOVE 100 PERCENT LOAD FROM BUS 7001 TO BUS 7002
END

COM --- 20. Block DC line ---
CONTINGENCY 'BLOCK_DC'
BLOCK TWOTERMDC 1
END

COM --- 21. Multi-event: branch + remove generator ---
CONTINGENCY 'MULTI_EVENT' CATEGORY 'CAT_N2'
DISCONNECT BRANCH FROM BUS 1001 TO BUS 1002 CIRCUIT 1
REMOVE UNIT 1 FROM BUS 4001
DECREASE BUS 6001 GENERATION BY 50 MW
MOVE 100 PERCENT LOAD FROM BUS 7001 TO BUS 7002
END

COM --- 22. OPEN keyword with FORM typo (data-quality tolerance) ---
CONTINGENCY 'TYPO_FORM'
OPEN LINE FORM BUS 1001 TO BUS 1002 CKT 1
END

COM --- 23. Move generation between buses ---
CONTINGENCY 'MOVE_GENERATION'
MOVE 50 MW GENERATION FROM BUS 7001 TO BUS 7002
END

COM --- 24. Move shunt between buses ---
CONTINGENCY 'MOVE_SHUNT'
MOVE 25 MVAR SHUNT FROM BUS 7003 TO BUS 7004
END

COM --- 25. Move active load between buses ---
CONTINGENCY 'MOVE_ACTIVE_LOAD'
MOVE 30 MW ACTIVE LOAD FROM BUS 7001 TO BUS 7002
END

COM --- 26. Move reactive load between buses ---
CONTINGENCY 'MOVE_REACTIVE_LOAD'
MOVE 15 MVAR REACTIVE LOAD FROM BUS 7003 TO BUS 7004
END
