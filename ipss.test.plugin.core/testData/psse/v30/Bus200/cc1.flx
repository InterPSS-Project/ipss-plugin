C
C  PLANT-RELATED USER MODELS
C
      SUBROUTINE USRXXX(MC,SLOT,IT)
      INTEGER MC,SLOT,IT
      SELECT (IT)
      (OTHERWISE) CALL UMALRM(MC,IT)
      FIN
      RETURN
      END
C
C  LOAD-RELATED USER MODELS
C
      SUBROUTINE USRLOD(LD,SLOT,SLOT2,IT)
      INTEGER LD,SLOT,SLOT2,IT
      SELECT (IT)
      (OTHERWISE) CALL ULALRM(LD,IT)
      FIN
      RETURN
      END
C
C  LINE RELAY USER MODELS
C
      SUBROUTINE USRREL(KM,RI,SLOT,IT)
      INTEGER KM,RI,SLOT,IT
      SELECT (IT)
      (OTHERWISE) CALL URALRM(KM,IT)
      FIN
      RETURN
      END
