<?xml version="1.0" encoding="UTF-8"?>
<ipss:InterPSS xmlns:ipss="http://www.interpss.org/schema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.interpss.org/schema ../../../../ipss.xml/schema/InterPSS.xsd ">
	<ipss:runStudyCase>
		<ipss:analysisRunType>ContingencyAnalysis</ipss:analysisRunType>
		<ipss:contingencyAnalysis>
		<ipss:option>
			<ipss:busVLimitPU>
				<ipss:max>1.07</ipss:max>
				<ipss:min>0.9</ipss:min>
			</ipss:busVLimitPU>
		</ipss:option>
		<ipss:defaultAclfAlgorithm>
				<ipss:lfMethod>NR</ipss:lfMethod>
				<ipss:maxIterations>20</ipss:maxIterations>
				<ipss:tolerance>10.0</ipss:tolerance>
				<ipss:toleranceUnit>KVA</ipss:toleranceUnit>
				<ipss:initBusVoltage>true</ipss:initBusVoltage>
				<ipss:displaySummary>true</ipss:displaySummary>
				<ipss:accFactor>0.0</ipss:accFactor>
		</ipss:defaultAclfAlgorithm>
		<ipss:aclfStudyCaseList>
			<ipss:aclfStudyCase>
				<ipss:recId>RunAclfCaseTest_1</ipss:recId>
				<ipss:modification>
					<ipss:branchChangeRecList>
						<ipss:branchChangeRec>
							<ipss:recId>BranchChangeRec-1</ipss:recId>
							<ipss:fromBusId>0010</ipss:fromBusId>
							<ipss:toBusId>0009</ipss:toBusId>
							<ipss:offLine>true</ipss:offLine>
						</ipss:branchChangeRec>
					</ipss:branchChangeRecList>
				</ipss:modification>
			</ipss:aclfStudyCase>
			<ipss:aclfStudyCase>
				<ipss:recId>RunAclfCaseTest_2</ipss:recId>
				<ipss:modification>
					<ipss:branchChangeRecList>
						<ipss:branchChangeRec>
							<ipss:recId>BranchChangeRec-2</ipss:recId>
							<ipss:fromBusId>0005</ipss:fromBusId>
							<ipss:toBusId>0006</ipss:toBusId>
							<ipss:offLine>true</ipss:offLine>
						</ipss:branchChangeRec>
					</ipss:branchChangeRecList>
				</ipss:modification>
			</ipss:aclfStudyCase>
			<ipss:aclfStudyCase>
				<ipss:recId>RunAclfCaseTest_3</ipss:recId>
				<ipss:modification>
					<ipss:branchChangeRecList>
						<ipss:branchChangeRec>
							<ipss:recId>BranchChangeRec-3</ipss:recId>
							<ipss:fromBusId>0004</ipss:fromBusId>
							<ipss:toBusId>0007</ipss:toBusId>
							<ipss:offLine>true</ipss:offLine>
						</ipss:branchChangeRec>
					</ipss:branchChangeRecList>
				</ipss:modification>
			</ipss:aclfStudyCase>
		</ipss:aclfStudyCaseList>
		</ipss:contingencyAnalysis>
	</ipss:runStudyCase>
</ipss:InterPSS>
