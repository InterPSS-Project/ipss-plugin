<?xml version="1.0" encoding="UTF-8"?>
<ipss:InterPSS xmlns:ipss="http://www.interpss.org/schema" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.interpss.org/schema ../../../../ipss.xml/schema/InterPSS.xsd">
	<ipss:runStudyCase>
		<ipss:analysisRunType>RunAclf</ipss:analysisRunType>
		<ipss:gridRunOption>
			<ipss:enableGridRun>false</ipss:enableGridRun>
			<ipss:remoteJobCreation>false</ipss:remoteJobCreation>
			<ipss:timeout>0</ipss:timeout>
		</ipss:gridRunOption>
 		<ipss:standardRun>
		<ipss:runAclfStudyCase>
			<ipss:defaultAclfAlgorithm>
				<ipss:lfMethod>NR</ipss:lfMethod>
				<ipss:maxIterations>20</ipss:maxIterations>
				<ipss:tolerance>10.0</ipss:tolerance>
				<ipss:toleranceUnit>KVA</ipss:toleranceUnit>
				<ipss:initBusVoltage>true</ipss:initBusVoltage>
				<ipss:displaySummary>true</ipss:displaySummary>
			</ipss:defaultAclfAlgorithm>
		<ipss:aclfStudyCaseList>
			<ipss:aclfStudyCase>
				<ipss:recId>RunAclfCaseTest_1</ipss:recId>
				<ipss:modification>
					<ipss:busChangeRecList>
						<ipss:busChangeRec>
							<ipss:recId>Addloadatbus12</ipss:recId>
							<ipss:busId>Bus12</ipss:busId>
							<ipss:aclfBusChangeData>
								<ipss:loadChangeData>
									<ipss:valueChange>
										<ipss:changeAction>increase</ipss:changeAction>
										<ipss:percent>10.0</ipss:percent>
										<ipss:unit>Percent</ipss:unit>
									</ipss:valueChange></ipss:loadChangeData></ipss:aclfBusChangeData>
						</ipss:busChangeRec></ipss:busChangeRecList></ipss:modification>
			</ipss:aclfStudyCase>
			<ipss:aclfStudyCase>
				<ipss:recId>RunAclfCaseTest_2</ipss:recId>				
			</ipss:aclfStudyCase>
			</ipss:aclfStudyCaseList>
		</ipss:runAclfStudyCase>
 		</ipss:standardRun>
	</ipss:runStudyCase>
</ipss:InterPSS>
